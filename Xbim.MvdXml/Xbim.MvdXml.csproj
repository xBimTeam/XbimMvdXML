<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net472</TargetFrameworks>
    <PackageId>Xbim.MvdXML</PackageId>
    <Version>5.1.2</Version>
    <Authors>Claudio Benghi</Authors>
    <Company>xBIM Team</Company>
    <Product>Xbim.MvdXML</Product>
    <Description>Library implementing buildingSMART standard for Model View Definitions (MVD XML). This library can be used to create and read mvdXML definitions. Most importantly it can be used to validate IFC files for compliance with these MVD definitions.</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Refactored to the latest xBIM libraries with independent logging implementation using Microsoft abstractions.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/xBimTeam/XbimMvdXML</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <RepositoryUrl>https://github.com/xBimTeam/XbimMvdXML</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>BIM, IFC, MVD, mvdxml, model checking, model validation, buildingSMART</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseExpression>CDDL-1.1</PackageLicenseExpression>
    <PackageIcon>Icon.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Properties\**" />
    <EmbeddedResource Remove="Properties\**" />
    <None Remove="Properties\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Xbim.Ifc" Version="5.1.329" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\Icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
